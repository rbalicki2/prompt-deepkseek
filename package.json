  "version": "0.0.1",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.promptDeepseek"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
